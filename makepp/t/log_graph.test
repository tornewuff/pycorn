# Fan out into one process per output format.
$::all_types = sub {
  my( $outfile, $type ) = @_;
  if( ::is_windows > 1 || !fork ) {
    $$type = 'txt';
    $outfile =~ s/udg$/txt/;
    open STDOUT, '>', $outfile or die "can't write `$outfile'--$!";
  } elsif( !fork ) {
    $$type = 'html';
    $outfile =~ s/udg$/html/;
    open STDOUT, '>', $outfile or die "can't write `$outfile'--$!";
  } elsif( !fork ) {
    $$type = 'dot';
    $outfile =~ s/udg$/dot/;
    open STDOUT, '>', $outfile or die "can't write `$outfile'--$!";
  } else {
    1 while -1 < wait;
  }
};

